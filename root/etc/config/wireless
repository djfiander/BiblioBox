
config wifi-device 'radio0'
	option type 'mac80211'
	option macaddr '00:24:a5:d8:d0:c7'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'DSSS_CCK-40'
	option txpower '27'
	option country 'US'
	option channel '8'
	option hwmode '11g'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Erebus'
	option encryption 'psk-mixed'
	option key 'Speak "friend" and enter'

